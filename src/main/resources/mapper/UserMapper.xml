<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yanle.mapper.UserMapper">
    <select id="selectByLoginName" parameterType="com.yanle.entity.UserVo" resultType="com.yanle.entity.User" >
        SELECT * FROM  USER  where login_name = #{loginName}
    </select>
</mapper>